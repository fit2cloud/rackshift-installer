default-lease-time 600;
max-lease-time 7200;

# authoritative;
# log-facility local7;

ignore-client-uids true;
deny duplicates;

option domain-name-servers 172.31.128.250;
option routers 172.31.128.254;

subnet ipsegment netmask netmask0 {
  range startip endip;
  option vendor-class-identifier "PXEClient";
}
